/**app.wxss**/
@import 'style/weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.block {
  height:30rpx;
  width:100%;
  background-color: #F8F8F8;
  display: block;
}
.radio {
  margin-right: 10rpx;
  height: 2em;
  min-height: 2em;
  line-height: 2em;
}